<?xml version="1.0" encoding="UTF-8"?>
	
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
          version="2.2">

	<application>
		<locale-config>
			<default-locale>es</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>
				es.everis.gvias.recursos.mensajes
			</base-name>
			<var>mensajes</var>
		</resource-bundle>
		<el-resolver>
			org.primefaces.application.exceptionhandler.PrimeExceptionHandlerELResolver
		</el-resolver>
	</application>

	<factory>
		<exception-handler-factory>
			org.primefaces.application.exceptionhandler.PrimeExceptionHandlerFactory
		</exception-handler-factory>
	</factory>
	<navigation-rule>
		<navigation-case>
			<from-outcome>bienvenida</from-outcome>
			<to-view-id>/paginas/inicio/bienvenida.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>listarViasCategorizacion</from-outcome>
			<to-view-id>/paginas/via/listadoViasCategorizacion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>listarViasPlanVial</from-outcome>
			<to-view-id>/paginas/via/listadoViasPlanVial.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>categorizarVia</from-outcome>
			<to-view-id>/paginas/via/categorizarVia.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>planVialVia</from-outcome>
			<to-view-id>/paginas/via/planVialVia.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>administracion</from-outcome>
			<to-view-id>/paginas/administracion/administracion.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>analisis</from-outcome>
			<to-view-id>/paginas/administracion/analisis.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<navigation-case>
			<from-outcome>mapa</from-outcome>
			<to-view-id>/paginas/mapa/mapa.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>